    "cnode__b_false_false" -> "lnode__b_false_false"
    "lnode__b_false_false" -> "lnode__b_false_false_subs"
    { rank=same; "pnode__b_fake" }
    { rank=same; "cnode__b_false_true", "cnode__b_false_false" }
